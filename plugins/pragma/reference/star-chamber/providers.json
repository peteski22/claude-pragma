{
  "platform": "any-llm",
  "providers": [
    {
      "provider": "openai",
      "model": "gpt-5.2",
      "api_key": "${OPENAI_API_KEY}",
      "max_tokens": 128000
    },
    {
      "provider": "anthropic",
      "model": "claude-opus-4-6",
      "api_key": "${ANTHROPIC_API_KEY}",
      "max_tokens": 8192
    },
    {
      "provider": "gemini",
      "model": "gemini-2.5-flash",
      "api_key": "${GEMINI_API_KEY}",
      "max_tokens": 65536
    },
    {
      "provider": "llamafile",
      "model": "local-model",
      "api_base": "http://localhost:8080/v1",
      "max_tokens": 4096,
      "local": true
    }
  ],
  "consensus_threshold": 2,
  "timeout_seconds": 120
}
